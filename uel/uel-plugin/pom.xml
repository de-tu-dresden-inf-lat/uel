<project xmlns="http://maven.apache.org/POM/4.0.0"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">


	<!--
 *
 * @author Julian Mendez
 *
-->

	<modelVersion>4.0.0</modelVersion>

	<groupId>uel</groupId>
	<artifactId>uel-plugin</artifactId>
	<version>1.3.0</version>
	<packaging>jar</packaging>

	<name>uel Protege plug-in</name>


	<properties>
		<project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
	</properties>


	<licenses>

		<license>
			<name>GNU Lesser General Public License version 3</name>
			<url>http://www.gnu.org/licenses/lgpl.txt</url>
		</license>

		<license>
			<name>Apache License, Version 2.0</name>
			<url>http://www.apache.org/licenses/LICENSE-2.0.txt</url>
		</license>

	</licenses>

	<build>
		<plugins>
			<plugin>
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-javadoc-plugin</artifactId>
				<version>2.7</version>
				<configuration>
					<includeDependencySources>true</includeDependencySources>
					<dependencySourceIncludes>
						<dependencySourceInclude>de.tudresden.inf.lat.uel.*</dependencySourceInclude>
					</dependencySourceIncludes>
				</configuration>
			</plugin>
		</plugins>
	</build>

	<dependencies>

		<dependency>
			<groupId>uel</groupId>
			<artifactId>uel-type</artifactId>
			<version>1.3.0</version>
		</dependency>

		<dependency>
			<groupId>uel</groupId>
			<artifactId>uel-rule</artifactId>
			<version>1.3.0</version>
		</dependency>

		<dependency>
			<groupId>uel</groupId>
			<artifactId>uel-sat</artifactId>
			<version>1.3.0</version>
		</dependency>

		<dependency>
			<groupId>uel</groupId>
			<artifactId>uel-asp</artifactId>
			<version>1.3.0</version>
		</dependency>

		<dependency>
			<groupId>net.sourceforge.owlapi</groupId>
			<artifactId>owlapi</artifactId>
			<version>3.4.2</version>
		</dependency>

		<dependency>
			<groupId>org.protege</groupId>
			<artifactId>protege-editor-owl</artifactId>
			<version>4.1</version>
		</dependency>

		<dependency>
			<groupId>org.protege</groupId>
			<artifactId>protege-editor-core-application</artifactId>
			<version>4.1</version>
		</dependency>

		<dependency>
			<groupId>com.sun.java</groupId>
			<artifactId>jlfgr</artifactId>
			<version>1.0</version>
		</dependency>

		<dependency>
			<groupId>junit</groupId>
			<artifactId>junit</artifactId>
			<version>4.11</version>
		</dependency>

		<dependency>
			<groupId>de.tudresden.inf.lat.jcel</groupId>
			<artifactId>jcel</artifactId>
			<version>0.19.1</version>
		</dependency>

	</dependencies>

	<repositories>

		<repository>
			<id>localrepo</id>
			<name>local maven repository</name>
			<url>file:../uel-build/jars/lib</url>
		</repository>

	</repositories>

</project>

